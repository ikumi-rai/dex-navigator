{
  "manifest_version": 3,
  "name": "Dex Navigator",
  "version": "1.0.0",
  "description": "Add links to various DEXs and platforms in the context menu.",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "service_worker": "service_worker.js"
  }
}
